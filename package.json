    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "css-loader": "^0.25.0",
    "express": "^4.14.1",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.2.0",
